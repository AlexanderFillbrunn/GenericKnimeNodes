<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE knimeNode PUBLIC "-//UNIKN//DTD KNIME Node 2.0//EN" "http://www.knime.org/Node.dtd">
<knimeNode icon="./default.png" type="LoopStart">
    <name>ListZipLoopStart</name>
    
    <shortDescription></shortDescription>
    
    <fullDescription>
        <intro>Gives in the i-th iteration step the i-th element of the MIMEFile list from k-th inpot port at the k-th output port.</intro>
    </fullDescription>
    
    <ports>
     <inPort index="0" name="mimefile"> MIMEFiles </inPort>
     <inPort index="1" name="mimefile"> MIMEFiles (optional) </inPort>
     <inPort index="2" name="mimefile"> MIMEFiles (optional) </inPort>
     <inPort index="3" name="mimefile"> MIMEFiles (optional) </inPort>
     <outPort index="0" name="table"> i-th MIMEFile from port 0 in i-th iteration </outPort>
     <outPort index="1" name="table"> i-th MIMEFile from port 1 in i-th iteration  (optional)</outPort>   
     <outPort index="2" name="table"> i-th MIMEFile from port 2 in i-th iteration  (optional)</outPort>
     <outPort index="3" name="table"> i-th MIMEFile from port 3 in i-th iteration  (optional)</outPort>
    </ports>    
    <views>
        <view index="0" name=""></view>
    </views>
</knimeNode>