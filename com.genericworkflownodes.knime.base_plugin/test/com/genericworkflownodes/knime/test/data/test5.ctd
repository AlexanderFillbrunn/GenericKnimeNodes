<?xml version="1.0" encoding="UTF-8"?>
<tool status="external" xsi:noNamespaceSchemaLocation="http://page.mi.fu-berlin.de/aiche/schemas/CTD.xsd"   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<name>BLAST</name>
	<version>2.2.1</version>
	<description><![CDATA[BLAST.]]></description>
	<manual><![CDATA[BLAST.]]></manual>
	<docurl>http://www.google.de</docurl>
	<category>Sequence Tools</category>
	<cli>
		<clielement optionIdentifier="-i" isList="true">
			<mapping referenceName="blastall.i" />
		</clielement>
		<clielement optionIdentifier="-d">
			<mapping referenceName="blastall.d" />
		</clielement>
	</cli>
	<output-converters>
		<converter class="DummyConverter" ref="blastall.o" />
		<converter class="DummyConverter2" ref="blastall.o">
			<converter-property name="prop1" value="val1" />
			<converter-property name="prop2" value="val2" />
		</converter>
	</output-converters>
	<PARAMETERS version="1.3"
		xsi:noNamespaceSchemaLocation="http://open-ms.sourceforge.net/schemas/Param_1_3.xsd"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
		<NODE name="blastall" description="">
			<ITEM name="i" value="" type="string" description="input file"
				tags="input file,required" restrictions="*.FASTA" />
			<ITEM name="o" value="" type="string" description="output file"
				tags="output file,required" restrictions="*.BLASTXML" />
			<ITEM name="p" value="blastp" type="string" description="mode"
				restrictions="blastp,blastn" />
			<ITEM name="d" value="" type="string" tags="required"
				description="database path" restrictions="" />
			<ITEM name="e" value="0.001" type="float" description="cutoff evalue"
				restrictions="" />
			<ITEM name="G" value="-1" type="float" description="gap opening"
				restrictions="" />
			<ITEM name="E" value="-1" type="float" description="gap extension"
				restrictions="" />
			<ITEM name="M" value="BLOSUM62" type="string" description="scoring matrix"
				restrictions="BLOSUM62,BLOSUM45" />
		</NODE>
	</PARAMETERS>
</tool>